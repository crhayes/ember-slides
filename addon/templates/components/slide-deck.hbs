{{yield (hash
  slide=(component "slide-card"
    activeSlide=(readonly activeSlide)
    registerSlide=(action "registerSlide")
    unregisterSlide=(action "unregisterSlide")
  )
  slides=(readonly yieldedSlides)
  activeSlide=(readonly activeSlide)
  onFirstSlide=(readonly onFirstSlide)
  onLastSlide=(readonly onLastSlide)
  prevDisabled=(readonly prevDisabled)
  nextDisabled=(readonly nextDisabled)
  prev=(action "prev")
  next=(action "next")
  show=(action "show")
)}}
